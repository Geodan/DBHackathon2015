<Window x:Class="Blackbird.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xaml="clr-namespace:Mapsui.UI.Xaml;assembly=Mapsui.UI.Xaml"
        Title="Blackbird" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <xaml:MapControl Grid.Row="1" Grid.Column="0" x:Name="MapControl" Stylus.IsPressAndHoldEnabled="False" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
    </Grid>
</Window>
